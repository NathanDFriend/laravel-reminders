{
    "name": "alariva/laravel-reminders",
    "description": "Add reminders to your Laravel models",
    "keywords": [
        "alariva",
        "laravel-reminders"
    ],
    "homepage": "https://github.com/alariva/laravel-reminders",
    "license": "MIT",
    "authors": [
        {
            "name": "Ariel Vallese",
            "email": "alariva@gmail.com",
            "homepage": "https://alariva.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.0",
        "laravel/framework": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2|^7.0",
        "orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
    },
    "autoload": {
        "psr-4": {
            "Alariva\\Reminders\\": "src"
        }
    },

    "autoload-dev": {
        "psr-4": {
            "Alariva\\Reminders\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alariva\\Reminders\\RemindersServiceProvider"
            ]
        }
    }
}
